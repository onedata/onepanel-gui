<div class="row content-row row-medium{{if (not importConfigurationOpen) " row-hidden"}}">
  <h1 class="form-title">{{tt this "syncFormHeader"}}</h1>
  {{storage-import-update-form
    class="col-xs-12 col-sm-10 col-md-8 col-lg-6 col-centered"
    defaultValues=space
    mode=(if importEnabled "edit" "new")
    isFormOpened=importConfigurationOpen
    submit=(action "modifySpace")
  }}
</div>