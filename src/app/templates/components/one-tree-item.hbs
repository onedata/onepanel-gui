<div class="tree-circle"></div>
{{yield (hash
  content=(component 'one-tree-item-content' _showAction=(action "show" key))
  subtree=(component 'one-tree' 
    key=key
    collapseRecursively=collapseRecursively
    _isExpanded=_isSubtreeExpanded
    _showAction=(action "show")
    _parentKey=_parentKey
    _rootKey=_rootKey
    _areParentsExpanded=_areParentsExpanded
    _activeSubtreeKeys=_activeSubtreeKeys)
  toggleAction=(action "show" key)
  collapseAction=(action "show" key false)
  expandAction=(action "show" key true)
)}}
